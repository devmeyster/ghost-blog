    <script type="text/javascript">
        $("nav > div > ul > li > a").click(function(event) {
            console.log("mix panel event", event);
            var cb = generate_callback($(this));
            event.preventDefault();
            mixpanel.track("Clicked Link", { "Domain": "arturmeyster.com" }, cb);
            setTimeout(cb, 500);
        })
            
        function generate_callback(a) {
            return function() {
                window.location = a.attr("href");
            }
        }
    </script>    
